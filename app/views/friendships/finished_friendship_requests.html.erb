<div align="center" class="container-fluid">
  <div align="left" style="width:300px;">
    <h1>已处理的好友申请</h1>
    <ul class="list-group">
      <% @finished_requests.each do |request| %>
      <li class="list-group-item">
        <% request.id %>
        <% applicant = User.find_by(id: request.user_id) %>
        <%= applicant.name %>
        ->
        <% friend = User.find_by(id: request.friend_id) %>
        <%= friend.name %>
      </li>
      <% end %>
    </ul>
  </div>
</div>
